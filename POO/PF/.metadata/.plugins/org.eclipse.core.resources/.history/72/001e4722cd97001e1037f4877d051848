package questao2;

import java.util.HashSet;
import java.util.Scanner;
import java.util.Set;

public class Utilidades 
{
	public static Set <String>getPalavroes() 
	{
		Set <String> p = new HashSet <String>();
		p.add("bonita");
		//p.add("feia");
		p.add("preta");
		p.add("abucetando");
		p.add("coco");
		p.add("Estrela");
		p.add("planeta");)
		return p;
	}

	
	public static void main(String[]args) throws TextoCurtoException
	{
		Scanner teclado = new Scanner(System.in);
		String texto = teclado.nextLine();
		teclado.close();
		try {
			Utils.verifica(texto);
		}
		catch (TextoCurtoException erro) {
			System.out.println("Texto inserido com menos de 10 caracteres" + erro);
		}
//		String t = "Minha casa Ã© bonita.";
//		t = "Mi"; //false t = " "; //false
//		System.out.println(t.contains(texto));
//		System.out.println(t);
//		System.out.println(t.length());
		Utils.moderar(texto);
	}
	
}
